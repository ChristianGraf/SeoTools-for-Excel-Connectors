          <Item Id="uk" Title="UK" />
          <Item Id="us" Title="USA" />
          <Item Id="se" Title="Sweden" />
        </DataSource>
      </Select>
    </Resource>
    <Resource Id="SelectMode">
      <Radio Id="Mode" Title="Mode" DefaultValue="domain">
